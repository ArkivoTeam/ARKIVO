                    <rdf:Description rdf:about="http://www.arkivo-ontology.org/ontology#HistoricalEvent"/>
                    <rdf:Description rdf:about="http://www.arkivo-ontology.org/ontology#Item"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:comment>Thing created  or named by people</rdfs:comment>
