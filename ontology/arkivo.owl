    <owl:Ontology rdf:about="http://www.arkivo-ontology.org/ontology">
        <owl:versionIRI rdf:resource="http://www.arkivo-ontology.org/ontology/1.0.0"/>
        <dcterms:description xml:lang="en">ARKIVO ontology is designed to accomodate archival description, including colelction hierarchy, digitized and born-digital items, historical events, names, places and other entities that exist in archives.</dcterms:description>
        <rdfs:label xml:lang="en">ARKIVO ontology</rdfs:label>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/description"/>
    


    <!-- http://www.w3.org/2002/07/owl#equivalentProperty -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#equivalentProperty"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2002/07/owl#sameAs"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="http://www.w3.org/2001/XMLSchema#date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:ObjectProperty rdf:about="http://purl.org/dc/terms/creator">
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#GlamThing"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
        <rdfs:comment xml:lang="en">An entity primarily responsible for making the resource.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/dc/terms/creator</rdfs:isDefinedBy>
        <owl:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">schema:creator</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/birthPlace -->

    <owl:ObjectProperty rdf:about="http://schema.org/birthPlace">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#bornIn"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <rdfs:comment xml:lang="en">The place where the person was born.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">schema:birthPlace</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">birth place</rdfs:label>
        <rdfs:label xml:lang="pl">miejsce urodzenia</rdfs:label>
        <owl:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://dbpedia.org/ontology/birthPlace</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/deathPlace -->

    <owl:ObjectProperty rdf:about="http://schema.org/deathPlace">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#diedIn"/>
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <rdfs:comment xml:lang="en">The place where the person died.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">schema:deathPlace</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">death place</rdfs:label>
        <owl:sameAs rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">dbo:deathPlace</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/hasPart -->

    <owl:ObjectProperty rdf:about="http://schema.org/hasPart">
        <owl:inverseOf rdf:resource="http://schema.org/isPartOf"/>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:range rdf:resource="http://www.arkivo-ontology.org/ontology#GlamThing"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://schema.org/</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/isPartOf -->

    <owl:ObjectProperty rdf:about="http://schema.org/isPartOf">
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#GlamThing"/>
        <rdfs:range rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://schema.org/</rdfs:isDefinedBy>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/mentions -->

    <owl:ObjectProperty rdf:about="http://schema.org/mentions">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#isMentionedIn"/>
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#CreativeThing"/>
        <rdfs:range rdf:resource="http://www.arkivo-ontology.org/ontology#NamedThing"/>
        <rdfs:label>mentions</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#bornIn -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#bornIn">
        <rdfs:domain rdf:resource="http://schema.org/Place"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:comment xml:lang="en">A person was born in this place</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#cites -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#cites">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#isCitedBy"/>
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#Item"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#createdDuring -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#createdDuring">
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#Item"/>
        <rdfs:range rdf:resource="http://www.arkivo-ontology.org/ontology#HistoricalPeriod"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#diedIn -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#diedIn">
        <rdfs:domain rdf:resource="http://schema.org/Place"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
        <rdfs:comment xml:lang="en">A person died in this place</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#happenedDuring -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#happenedDuring">
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="http://www.arkivo-ontology.org/ontology#HistoricalPeriod"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#involves -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#involves">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#isInvolvedIn"/>
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isAbout -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isAbout">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#isSubjectOf"/>
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#Item"/>
        <rdfs:range rdf:resource="http://purl.org/dc/elements/1.1/Subject"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isCitedBy -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isCitedBy"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#isInvolvedIn -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isInvolvedIn"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#isMentionedIn -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isMentionedIn">
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#NamedThing"/>
        <rdfs:range rdf:resource="http://www.arkivo-ontology.org/ontology#CreativeThing"/>
        <rdfs:comment>Object is mentioned in an item, collection, etc., without specifying the role. Use specific role-based properties (like dcterms:creator, birthDate, takesPlaceIn etc., if known)</rdfs:comment>
        <rdfs:label>is mentioned in</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isPrimaryTopicOf -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isPrimaryTopicOf">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#primaryTopic"/>
        <owl:sameAs>foaf:isPrimaryTopicOf</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isReportedIn -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isReportedIn">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#reports"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isSceneOf -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isSceneOf">
        <owl:inverseOf rdf:resource="http://www.arkivo-ontology.org/ontology#takesPlaceIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isSubjectOf -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#isSubjectOf"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#primaryTopic -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#primaryTopic">
        <owl:sameAs></owl:sameAs>
        <owl:sameAs>foaf:primaryTopic</owl:sameAs>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#reports -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#reports">
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#Item"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#repository -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#repository">
        <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
        <rdfs:range rdf:resource="http://www.arkivo-ontology.org/ontology#CreativeThing"/>
        <rdfs:comment xml:lang="en">An institution (e.g a library, archive, museum, gallery etc.) can be a repository of collections or items</rdfs:comment>
        <rdfs:label xml:lang="en">repository</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#takesPlaceIn -->

    <owl:ObjectProperty rdf:about="http://www.arkivo-ontology.org/ontology#takesPlaceIn">
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
    </owl:ObjectProperty>
    


