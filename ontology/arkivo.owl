    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedevents.org/ontology/atTime -->

    <owl:DatatypeProperty rdf:about="http://linkedevents.org/ontology/atTime">
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#date"/>
        <rdfs:comment xml:lang="en"> An instant or interval of time that is when an event happened.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://linkedevents.org/ontology/</rdfs:isDefinedBy>
        <rdfs:label xml:lang="en">atTime</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:DatatypeProperty rdf:about="http://purl.org/dc/elements/1.1/description">
        <rdfs:domain rdf:resource="http://www.arkivo-ontology.org/ontology#Item"/>
