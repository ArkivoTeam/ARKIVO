        <rdfs:comment xml:lang="en"> The Digital Object Identifier.</rdfs:comment>
        <rdfs:isDefinedBy rdf:datatype="http://www.w3.org/2001/XMLSchema#anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:label>doi</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/isbn -->

