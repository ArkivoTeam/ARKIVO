<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY dbo "http://dbpedia.org/ontology/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY arkivo "http://www.arkivo-ontology.org/ontology" >
    <!ENTITY ontology "http://www.arkivo-ontology.org/ontology#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="&arkivo;#"
     xml:base="http://www.arkivo-ontology.org/ontology"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:arkivo="http://www.arkivo-ontology.org/ontology"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:dbo="http://dbpedia.org/ontology/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ontology="&arkivo;#"
     xmlns:dcterms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://www.arkivo-ontology.org/ontology">
        <rdfs:label xml:lang="en">ARKIVO ontology</rdfs:label>
        <rdfs:seeAlso rdf:datatype="&xsd;anyURI"> https://github.com/ArkivoTeam/ARKIVO </rdfs:seeAlso>
        <owl:versionInfo rdf:datatype="&xsd;double">0.2</owl:versionInfo>
        <dcterms:issued rdf:datatype="&xsd;dateTime">2017-05-15</dcterms:issued>
        <rdfs:comment rdf:datatype="&xsd;string">ARKIVO ontology by Laura Pandolfo, Luca Pulina and Marel Zielinski is licensed under CC BY 4.0 (http://creativecommons.org/licenses/by/3.0/).</rdfs:comment>
        <dcterms:description rdf:datatype="&xsd;string">ARKIVO ontology is an ontology designed to accomodate the archival description, with inclusion of historical events.</dcterms:description>
        <dc:contributor rdf:datatype="&xsd;string">Laura Pandolfo, Luca Pulina, Marek Zielinski.</dc:contributor>
        <dcterms:license rdf:datatype="&xsd;anyURI">http://creativecommons.org/licenses/by/3.0/</dcterms:license>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/contributor -->

    <owl:AnnotationProperty rdf:about="&dc;contributor"/>
    


    <!-- http://purl.org/dc/terms/description -->

    <owl:AnnotationProperty rdf:about="&dcterms;description"/>
    


    <!-- http://purl.org/dc/terms/issued -->

    <owl:AnnotationProperty rdf:about="&dcterms;issued"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="&dcterms;license"/>
    


    <!-- http://www.w3.org/2002/07/owl#equivalentProperty -->

    <owl:AnnotationProperty rdf:about="&owl;equivalentProperty"/>
    


    <!-- http://www.w3.org/2002/07/owl#sameAs -->

    <owl:AnnotationProperty rdf:about="&owl;sameAs"/>
    


    <!-- http://www.w3.org/2002/07/owl#versionIRI -->

    <owl:AnnotationProperty rdf:about="&owl;versionIRI"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2001/XMLSchema#date -->

    <rdfs:Datatype rdf:about="&xsd;date"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/terms/creator -->

    <owl:ObjectProperty rdf:about="&dcterms;creator">
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/terms/creator</rdfs:isDefinedBy>
        <owl:sameAs rdf:datatype="&xsd;anyURI">schema:creator</owl:sameAs>
        <rdfs:comment xml:lang="en">An entity primarily responsible for making the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/birthPlace -->

    <owl:ObjectProperty rdf:about="http://schema.org/birthPlace">
        <rdfs:label xml:lang="en">birth place</rdfs:label>
        <rdfs:label xml:lang="pl">miejsce urodzenia</rdfs:label>
        <owl:sameAs rdf:datatype="&xsd;anyURI">http://dbpedia.org/ontology/birthPlace</owl:sameAs>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">schema:birthPlace</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">The place where the person was born.</rdfs:comment>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <owl:inverseOf rdf:resource="&arkivo;#bornIn"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/deathPlace -->

    <owl:ObjectProperty rdf:about="http://schema.org/deathPlace">
        <rdfs:label xml:lang="en">death place</rdfs:label>
        <owl:sameAs rdf:datatype="&xsd;anyURI">dbo:deathPlace</owl:sameAs>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">schema:deathPlace</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">The place where the person died.</rdfs:comment>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <owl:inverseOf rdf:resource="&arkivo;#diedIn"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/hasPart -->

    <owl:ObjectProperty rdf:about="http://schema.org/hasPart">
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://schema.org/</rdfs:isDefinedBy>
        <owl:inverseOf rdf:resource="http://schema.org/isPartOf"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/isPartOf -->

    <owl:ObjectProperty rdf:about="http://schema.org/isPartOf">
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://schema.org/</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
    </owl:ObjectProperty>
    


    <!-- http://schema.org/mentions -->

    <owl:ObjectProperty rdf:about="http://schema.org/mentions">
        <rdfs:label>mentions</rdfs:label>
        <rdfs:comment></rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#CreativeThing"/>
        <rdfs:range rdf:resource="&arkivo;#NamedThing"/>
        <owl:inverseOf rdf:resource="&arkivo;#isMentionedIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#bornIn -->

    <owl:ObjectProperty rdf:about="&arkivo;#bornIn">
        <rdfs:comment xml:lang="en">A person was born in this place</rdfs:comment>
        <rdfs:domain rdf:resource="http://schema.org/Place"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#cites -->

    <owl:ObjectProperty rdf:about="&arkivo;#cites">
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <owl:inverseOf rdf:resource="&arkivo;#isCitedBy"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#createdDuring -->

    <owl:ObjectProperty rdf:about="&arkivo;#createdDuring">
        <rdfs:range rdf:resource="&arkivo;#HistoricalPeriod"/>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#diedIn -->

    <owl:ObjectProperty rdf:about="&arkivo;#diedIn">
        <rdfs:comment xml:lang="en">A person died in this place</rdfs:comment>
        <rdfs:domain rdf:resource="http://schema.org/Place"/>
        <rdfs:range rdf:resource="&foaf;Person"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#happenedDuring -->

    <owl:ObjectProperty rdf:about="&arkivo;#happenedDuring">
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="&arkivo;#HistoricalPeriod"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#involves -->

    <owl:ObjectProperty rdf:about="&arkivo;#involves">
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <owl:inverseOf rdf:resource="&arkivo;#isInvolvedIn"/>
        <rdfs:range rdf:resource="&foaf;Agent"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isAbout -->

    <owl:ObjectProperty rdf:about="&arkivo;#isAbout">
        <rdfs:range rdf:resource="&dc;Subject"/>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <owl:inverseOf rdf:resource="&arkivo;#isSubjectOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isCitedBy -->

    <owl:ObjectProperty rdf:about="&arkivo;#isCitedBy"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#isInvolvedIn -->

    <owl:ObjectProperty rdf:about="&arkivo;#isInvolvedIn"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#isMentionedIn -->

    <owl:ObjectProperty rdf:about="&arkivo;#isMentionedIn">
        <rdfs:label>is mentioned in</rdfs:label>
        <rdfs:comment>Object is mentioned in an item, collection, etc., without specifying the role. Use specific role-based properties (like dcterms:creator, birthDate, takesPlaceIn etc., if known)</rdfs:comment>
        <rdfs:range rdf:resource="&arkivo;#CreativeThing"/>
        <rdfs:domain rdf:resource="&arkivo;#NamedThing"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isReportedIn -->

    <owl:ObjectProperty rdf:about="&arkivo;#isReportedIn">
        <owl:inverseOf rdf:resource="&arkivo;#reports"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isSceneOf -->

    <owl:ObjectProperty rdf:about="&arkivo;#isSceneOf">
        <owl:inverseOf rdf:resource="&arkivo;#takesPlaceIn"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isSectionOf -->

    <owl:ObjectProperty rdf:about="&arkivo;#isSectionOf">
        <rdfs:domain rdf:resource="&arkivo;#File"/>
        <rdfs:range rdf:resource="&arkivo;#Fonds"/>
        <owl:inverseOf rdf:resource="&arkivo;#sectionOf"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#isSubjectOf -->

    <owl:ObjectProperty rdf:about="&arkivo;#isSubjectOf"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#reports -->

    <owl:ObjectProperty rdf:about="&arkivo;#reports">
        <rdfs:range rdf:resource="http://schema.org/Place"/>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#repository -->

    <owl:ObjectProperty rdf:about="&arkivo;#repository">
        <rdfs:label xml:lang="en">repository</rdfs:label>
        <rdfs:comment xml:lang="en">An institution (e.g a library, archive, museum, gallery etc.) can be a repository of collections or items</rdfs:comment>
        <rdfs:range rdf:resource="&arkivo;#CreativeThing"/>
        <rdfs:domain rdf:resource="&foaf;Organization"/>
    </owl:ObjectProperty>
    


    <!-- http://www.arkivo-ontology.org/ontology#sectionOf -->

    <owl:ObjectProperty rdf:about="&arkivo;#sectionOf"/>
    


    <!-- http://www.arkivo-ontology.org/ontology#takesPlaceIn -->

    <owl:ObjectProperty rdf:about="&arkivo;#takesPlaceIn">
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="http://schema.org/Place"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://linkedevents.org/ontology/atTime -->

    <owl:DatatypeProperty rdf:about="http://linkedevents.org/ontology/atTime">
        <rdfs:label xml:lang="en">atTime</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://linkedevents.org/ontology/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en"> An instant or interval of time that is when an event happened.</rdfs:comment>
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/description -->

    <owl:DatatypeProperty rdf:about="&dc;description">
        <rdfs:label xml:lang="en">description</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/elements/1.1/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Description may include but is not limited to: an abstract, a table of contents, a graphical representation, or a free-text account of the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/extent -->

    <owl:DatatypeProperty rdf:about="&dc;extent">
        <rdfs:label>extent</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/terms/extent</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">The size or duration of the resource</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#CreativeThing"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:DatatypeProperty rdf:about="&dc;title">
        <rdfs:label xml:lang="en">title</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/elements/1.1/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">A name given to the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/alternative -->

    <owl:DatatypeProperty rdf:about="&dcterms;alternative">
        <rdfs:label xml:lang="en">alternative title</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/terms/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">An alternative name or title of the resource. Often used for a name in a different language</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:DatatypeProperty rdf:about="&dcterms;created">
        <rdfs:label xml:lang="en">date</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/terms/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Date of creation of the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/date -->

    <owl:DatatypeProperty rdf:about="&dcterms;date">
        <rdfs:label xml:lang="en">date</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/dc/terms/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">A point or period of time associated with an event in the lifecycle of the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/argued -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/argued">
        <rdfs:label xml:lang="en">date argued</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">The date on which a legal case is argued before a court.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/LegalDocument"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/doi -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/doi">
        <rdfs:label>doi</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en"> The Digital Object Identifier.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/isbn -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/isbn">
        <rdfs:label>isbn</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">This property identifies the International Standard Book Number.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/issn -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/issn">
        <rdfs:label>issn</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">This data property allows to insert the ISSN, which is an 8-digit code used to identify newspapers, journals, magazines and periodicals.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/issue -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/issue">
        <rdfs:label>issue</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">This property indicates the number of a single copy of a periodical.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/pageEnd -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/pageEnd">
        <rdfs:label xml:lang="en">page end</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">Last number page of a document.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/pageStart -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/pageStart">
        <rdfs:label xml:lang="en">page start</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">First number page of a document.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;int"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/ontology/bibo/volume -->

    <owl:DatatypeProperty rdf:about="http://purl.org/ontology/bibo/volume">
        <rdfs:label>volume</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/ontology/bibo/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">The number of a series of issues of a periodical, usually covering one calendar year.</rdfs:comment>
        <rdfs:domain rdf:resource="http://purl.org/ontology/bibo/Collection"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/vocab/bio/0.1/biography -->

    <owl:DatatypeProperty rdf:about="http://purl.org/vocab/bio/0.1/biography">
        <rdfs:label xml:lang="en">biography</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://purl.org/vocab/bio/0.1/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">An extended description or account of someone&apos;s life.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;string"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/abstract -->

    <owl:DatatypeProperty rdf:about="http://schema.org/abstract">
        <rdfs:label xml:lang="en">abstract</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://schema.org/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">A summary of the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/birthDate -->

    <owl:DatatypeProperty rdf:about="http://schema.org/birthDate">
        <rdfs:label xml:lang="en">Date of birth.</rdfs:label>
        <rdfs:label>birth date</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://schema.org/</rdfs:isDefinedBy>
        <rdfs:range rdf:resource="&xsd;date"/>
        <rdfs:domain rdf:resource="&foaf;Person"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/endDate -->

    <owl:DatatypeProperty rdf:about="http://schema.org/endDate">
        <rdfs:label xml:lang="en">end date</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://schema.org/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">This property indicates the end date of an event. </rdfs:comment>
        <rdfs:domain rdf:resource="http://linkedevents.org/ontology/Event"/>
        <rdfs:subPropertyOf rdf:resource="http://linkedevents.org/ontology/atTime"/>
        <rdfs:range rdf:resource="&xsd;date"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/inLanguage -->

    <owl:DatatypeProperty rdf:about="http://schema.org/inLanguage">
        <rdfs:label xml:lang="en">in language</rdfs:label>
        <rdfs:isDefinedBy rdf:datatype="&xsd;anyURI">http://schema.org/</rdfs:isDefinedBy>
        <rdfs:comment xml:lang="en">This property specifies the language of the document.</rdfs:comment>
        <rdfs:domain rdf:resource="&arkivo;#Item"/>
        <rdfs:range rdf:resource="&xsd;string"/>
    </owl:DatatypeProperty>
    


